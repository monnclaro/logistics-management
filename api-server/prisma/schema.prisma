generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model DeliveryItem {
  id String @id @default(uuid())

  carNumber     String
  dateTime      String
  tu            String
  carrier       String
  shipTo        String
  weight        String
  numberOfItems String
  pickingStatus String
  createdAt     DateTime @default(now())
}

model WarehouseItem {
  id            String   @id @default(uuid())
  product       String
  description   String
  sku           String
  weight        String
  pickingStatus String
  createdAt     DateTime @default(now())
}
